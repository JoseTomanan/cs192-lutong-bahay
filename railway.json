{
    "services": {
        "frontend": {
            "buildCommand": "npm ci && npm run build",
            "startCommand": "npm run preview",
            "root": "frontend"
        },
        "backend": {
            "buildCommand": "pip install -r requirements.txt",
            "startCommand": "gunicorn mybackend.wsgi:application --bind 0.0.0.0:8000",
            "root": "backend"
        }
    }
}